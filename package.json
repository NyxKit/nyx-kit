{
  "name": "nyx-kit",
  "homepage": "http://nyxkit.github.io/nyx-kit",
  "author": "Arne Decant <hello@arnedecant.be>",
  "version": "1.1.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "gh-pages -d storybook-static",
    "storybook:publish": "storybook build && gh-pages -d storybook-static",
    "prepublishOnly": "yarn build"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./classes": {
      "import": "./dist/classes.mjs",
      "types": "./dist/types/classes.d.ts"
    },
    "./components": {
      "import": "./dist/components.mjs",
      "types": "./dist/types/components.d.ts"
    },
    "./compositions": {
      "import": "./dist/compositions.mjs",
      "types": "./dist/types/compositions.d.ts"
    },
    "./directives": {
      "import": "./dist/directives.mjs",
      "types": "./dist/types/directives.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "types": "./dist/types/types.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "types": "./dist/types/utils.d.ts"
    },
    "./style.css": {
      "import": "./dist/assets/nyx-kit.css"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "URL": "https://github.com/NyxKit/nyx-kit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-docs": "^8.5.6",
    "@storybook/addon-essentials": "8.5.6",
    "@storybook/addon-interactions": "8.5.6",
    "@storybook/addon-onboarding": "8.5.6",
    "@storybook/blocks": "8.5.6",
    "@storybook/cli": "^8.5.6",
    "@storybook/test": "8.5.6",
    "@storybook/vue3": "8.5.6",
    "@storybook/vue3-vite": "8.5.6",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-plugin-oxlint": "^0.15.6",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-storybook": "^0.11.3",
    "eslint-plugin-vue": "^9.32.0",
    "gh-pages": "^6.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.6",
    "sass": "^1.85.0",
    "storybook": "8.5.6",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "vue": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended",
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  }
}
